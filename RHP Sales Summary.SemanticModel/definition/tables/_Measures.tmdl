	measure 'Previous Month Sales' =
			IF(NOT (ISBLANK([Total Sales]))
			                            ,CALCULATE([Total Sales]
			                            ,PREVIOUSMONTH('Date'[Date]))
			                        )
		formatString: \$#,0.00;(\$#,0.00);\$#,0.00
		displayFolder: MoM
		lineageTag: ed9cc2d8-bf67-4c73-921f-2c5cc1155383

